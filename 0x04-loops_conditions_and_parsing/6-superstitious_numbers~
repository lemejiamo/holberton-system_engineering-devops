#!/usr/bin/env bash
# Script for writing numbers up to 20.

i=1
while [ $i -le 20 ]
do
    echo $i
    case $i in
	4)
	    echo bad luck from China
	    let i++
	    ;;
	9)
	    echo bad luck from Japan
	    let i++
	    ;;
	17)
	    echo bad luck from Italy
	    let i++
	    ;;
    esac
    let i++
done
